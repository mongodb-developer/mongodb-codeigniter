{
    "name": "codeigniter4/appstarter",
    "type": "project",
    "description": "MongoDB & CodeIgniter CRUD Tutorial",
    "homepage": "https://github.com/mongodb-developer/mongodb-codeigniter",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4",
        "mongodb/mongodb": "^1.12"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "forum": "https://github.com/mongodb-developer/mongodb-codeigniter/issues",
        "source": "https://github.com/mongodb-developer/mongodb-codeigniter"
    }
}
